{
	"name": "Move Data",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Dataset_Source_Sales",
						"type": "DatasetReference"
					},
					"name": "DataSource"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Dataset_Destination_Sales",
						"type": "DatasetReference"
					},
					"name": "LoadInBronze"
				}
			],
			"transformations": [
				{
					"name": "AddExecutionDateandTimeColumn"
				}
			],
			"scriptLines": [
				"source(output(",
				"          order_id as string,",
				"          customer_name as string,",
				"          product_name as string,",
				"          quantity as string,",
				"          unit_price as string,",
				"          order_date as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> DataSource",
				"DataSource derive(Processing_DateTime = currentDate()) ~> AddExecutionDateandTimeColumn",
				"AddExecutionDateandTimeColumn sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> LoadInBronze"
			]
		}
	}
}