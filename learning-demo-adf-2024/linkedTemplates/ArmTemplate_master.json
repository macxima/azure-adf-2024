{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "learning-demo-adf-2024"
		},
		"abs_linkservice_csv_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'abs_linkservice_csv'"
		},
		"adls_destination_zone_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_destination_zone'"
		},
		"adls_destination_zone_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://rastorageaccount2024.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "learning-demo-adf-2024_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"abs_linkservice_csv_connectionString": {
						"value": "[parameters('abs_linkservice_csv_connectionString')]"
					},
					"adls_destination_zone_accountKey": {
						"value": "[parameters('adls_destination_zone_accountKey')]"
					},
					"adls_destination_zone_properties_typeProperties_url": {
						"value": "[parameters('adls_destination_zone_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}